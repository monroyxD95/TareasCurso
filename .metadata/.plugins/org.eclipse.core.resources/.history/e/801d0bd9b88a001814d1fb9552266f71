package until;

import org.openqa.selenium.By;

public class Locators {
	public static final By USER_FIELD_LOCATOR= By.id("email");
	public static final By PASS_FIELD_LOCATOR= By.id("pass");
	public static final By BUTTON_FIELD_LOCATOR= By.xpath("//label[@id='loginbutton']//input");
	public static final By BUTTON_SEARCH_LOCATOR= By.xpath("//button[@class='_42ft _4jy0 _4w98 _4jy3 _517h _51sy _4w97']");
	public static final By CAMPO_SEARCH_LOCATOR= By.xpath("//input[@class='_1frb']");
	public static final By LINK_PERFILFRIEND_LOCATOR= By.xpath("//div[@class='_52eh _5bcu']");
	public static final By ADD_LIKE_LOCATOR= By.xpath("//button[@class='likeButton _4jy0 _4jy4 _517h _51sy _42ft']");
	public static final By ADD_FOTO_LOCATOR= By.xpath("//button[@href='/LosMensajerosUrbanos/photos/?ref=page_internal']");
	public static final By CONT_FOTOS_LOCATOR= By.xpath("//div[@class='_2eec']//img");
	
}
