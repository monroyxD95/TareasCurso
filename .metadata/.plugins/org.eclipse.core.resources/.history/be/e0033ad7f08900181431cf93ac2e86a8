package src1.tareas;

import libs.Input;

public class Botellas {
	public static void main(String[] args) {
		int numB = 0;
		int bot=0;
		 Input.print("Ingresa numero de botellas ");
	        numB = Input.get_int();
		bot=numB*12;
		Input.print("MINUTOS:"+numB+"\n");
		Input.print("BOTELLAS"+bot+"\n");
		
	}
}